{
  "name": "hz-core",
  "version": "0.0.3",
  "description": "HZ's internal library of React Components",
  "main": "index.js",
  "repository": "git@github.com:hzdg/hz-core.git",
  "author": "elainen <elaineh@hzdg.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/lerna bootstrap --loglevel=error && yarn build-packages",
    "build-docz": "yarn build && ./node_modules/.bin/docz build",
    "build-packages": "./node_modules/.bin/build-packages",
    "clean": "yarn build-packages --clean",
    "commit": "./node_modules/.bin/git-cz",
    "create-package": "./node_modules/.bin/create-package",
    "develop": "yarn build-packages && ./node_modules/.bin/docz dev",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./packages",
    "preversion": "yarn test && yarn build -f",
    "start": "yarn develop",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "test": "yarn build && ./node_modules/.bin/jest",
    "test-debug": "yarn build && node --inspect-brk ./node_modules/.bin/jest",
    "test-publish": "yarn build && ./node_modules/.bin/test-publish",
    "test-publish-debug": "yarn build && node --inspect-brk ./node_modules/.bin/test-publish"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "commit-template",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "registry": "http://npmregistry.hzdg.com"
  },
  "workspaces": [
    "packages/animations/*",
    "packages/behaviors/*",
    "packages/conventional-commits/*",
    "packages/simple-actions/*",
    "packages/utilities/*",
    "packages/ui-components/*"
  ],
  "config": {
    "commitizen": {
      "path": "@hzcore/commitizen-config"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/d3-array": "^2.0.0",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/debug": "^4.1.3",
    "@types/glob": "^7.1.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^24.0.22",
    "@types/jsdom": "12.2.4",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/react-motion": "^0.0.28",
    "@types/shallowequal": "^1.1.1",
    "@types/warning": "^3.0.0",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@vx/axis": "0.0.193-alpha.1",
    "@vx/curve": "0.0.193-alpha.1",
    "@vx/event": "0.0.193-alpha.1",
    "@vx/grid": "0.0.193-alpha.1",
    "@vx/group": "0.0.193-alpha.1",
    "@vx/legend": "0.0.193-alpha.2",
    "@vx/responsive": "0.0.193-alpha.1",
    "@vx/scale": "0.0.193-alpha.1",
    "@vx/shape": "0.0.193-alpha.1",
    "@vx/tooltip": "0.0.193-alpha.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "chalk": "^2.4.2",
    "commit-template": "^1.0.0",
    "commitizen": "^3.0.7",
    "d3-array": "^2.4.0",
    "d3-scale-chromatic": "^1.5.0",
    "docz": "^1.1.0",
    "docz-core": "^1.1.0",
    "docz-plugin-netlify": "^1.1.4",
    "docz-theme-default": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-hzdg": "^2.1.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-typescript": "^0.14.0",
    "glob": "^7.1.2",
    "husky": "^3.0.5",
    "invariant": "^2.2.4",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fifteen": "^1.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "jest-watch-yarn-workspaces": "^1.1.0",
    "lerna": "^3.13.1",
    "ncp": "^2.0.0",
    "netlify-cli": "^2.9.1",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-motion": "^0.5.2",
    "react-spring": "^8.0.15",
    "semver": "^6.0.0",
    "symbol-iterator": "^1.0.2",
    "typescript": "^3.7.4",
    "yargs": "^13.2.2"
  },
  "resolutions": {
    "docz-plugin-netlify/**/docz-core": "^1.1.0",
    "@octokit/endpoint": "4.0.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3"
  }
}
